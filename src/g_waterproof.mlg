DECLARE PLUGIN "coq-waterproof.plugin"

{

open Ltac_plugin
open Pp
open Stdarg

open Wauto

}

VERNAC COMMAND EXTEND Hello CLASSIFIED AS QUERY
  | [ "hello" ] ->
    {
      Feedback.msg_notice (strbrk "Hello world!")
    }
END

TACTIC EXTEND tactid
  | [ "tactid" ident(i) ] ->
    {
      test(i)
    }
END